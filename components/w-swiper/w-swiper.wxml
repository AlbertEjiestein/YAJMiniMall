<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper"
        autoplay="true"
        circular="true"
        interval="3000"
        duration="300"
        indicator-dots="true"
        indicator-active-color="#ff5777">
  <block wx:for="{{images}}" wx:key="index">
    <swiper-item>
      <image class="swiper-image" src="{{item}}" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>
